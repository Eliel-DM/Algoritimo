algoritmo "semnome"
var

     KM, DIA ,T : Real
inicio

      Escreval("Qual quantidade de KM percorridos?")
      Leia(KM)
      Escreval("Qual quantidade de dias o carro foi usado?")
      Leia(DIA)
      
      T <- (KM * 0.20) + (DIA * 90)
      Escreval("O valor total será de :",T," REAIS")
      

fimalgoritmo