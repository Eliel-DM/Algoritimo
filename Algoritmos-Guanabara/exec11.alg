algoritmo "semnome"
var
     A,B,C,DELTA: Real
inicio
      Escreval("Digite o valor do A!")
      Leia(A)
      Escreval("Digite o valor do B!")
      Leia(B)
      Escreval("Digite o valor do C!")
      Leia(C)
             DELTA <- ((B*B)-4*(A*C))
      Escreval("O valor do delta será de:" ,Delta)
fimalgoritmo
